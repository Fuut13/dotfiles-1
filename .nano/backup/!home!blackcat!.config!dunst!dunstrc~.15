[global]
    monitor = 0
    follow = mouse
    geometry = "350x5-15+20"
    indicate_hidden = yes
    shrink = yes
    transparency = 0
    separator_height = 2
    padding = 12
    horizontal_padding = 12
    frame_width = 2
    separator_color = auto
    frame_color = "#141212"
    sort = yes
    idle_threshold = 120
    font = artwiz lemon 8
    format = "<b>%s</b>\n%b"
    show_age_threshold = 60
    word_wrap = yes
    ignore_newline = no
    stack_duplicates = false
    hide_duplicate_count = false
    show_indicators = no
    icon_position = left
    max_icon_size = 0
    icon_folders = /usr/share/icons/paper/16x16/status/:/usr/share/icons/paper/16x16/devices/:/usr/share/icons/paper/16x16/devices/
    history_length = 20
    dmenu = /usr/bin/dmenu -p dunst:
    browser = /usr/bin/firefox -new-tab
    always_run_script = true

[shortcuts]
    close = ctrl+space
    close_all = ctrl+shift+space
    history = ctrl+grave
    context = ctrl+shift+period

[urgency_low]
    background = "#051d23"
    foreground = "#b5b5bc"
    timeout = 2
    icon = /usr/share/icons/Paper/16x16/status/dialog-info.png

[urgency_normal]
    background = "#051d23"
    foreground = "#b5b5bc"
    timeout = 4
    icon = /usr/share/icons/Paper/16x16/status/dialog-warning.png

[urgency_critical]
    background = "#051d23"
    foreground = "#b5b5bc"
    timeout = 0
    icon = /usr/share/icons/Paper/16x16/status/dialog-error.png
