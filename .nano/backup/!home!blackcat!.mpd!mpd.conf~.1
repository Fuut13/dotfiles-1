music_directory     "~/Music/Music"
playlist_directory  "~/Music/Music"
db_file             "~/.mpd/mpd.db"
log_file            "~/.mpd/mpd.log"
pid_file            "~/.mpd/mpd.pid"
state_file          "~/.mpd/mpdstate"
user				"blackcat"
bind_to_address     "localhost"
port                "6600"


audio_output {
				type "alsa"
				name "MyALSA"
				mixer_type "software"
				mixer_device "default"
				mixer_control  "PCH"
}

audio_output {
				type               "fifo"
				name               "MyFIFO"
				path               "/tmp/mpd.fifo"
				format             "44100:16:2"
}
